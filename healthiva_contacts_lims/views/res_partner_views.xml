          <filter string="Mother's Identifier" name="group_mother_identifier" context="{'group_by': 'mother_identifier'}"/>
          <filter string="Patient Address Line 1" name="group_patient_address1" context="{'group_by':'patient_address1'}"/>
          <filter string="Patient Address Line 2" name="group_patient_address2" context="{'group_by':'patient_address2'}"/>
          <filter string="Patient Address City" name="group_patient_address_city" context="{'group_by':'patient_address_city'}"/>
          <filter string="Patient Address State" name="group_patient_address_state" context="{'group_by':'patient_address_state'}"/>
          <filter string="Patient Address zip" name="group_patient_address_zip" context="{'group_by':'patient_address_zip'}"/>
        </xpath>
      </field>
