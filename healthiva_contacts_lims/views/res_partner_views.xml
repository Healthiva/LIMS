          <field name="patient_address_zip"/>
        </xpath>
        <xpath expr="//search/separator[1]" position="after">
          <filter string="First Name" name="first_name"/>
          <filter string="Middle Name" name="middle_name"/>
          <filter string="Last Name" name="last_name"/>
