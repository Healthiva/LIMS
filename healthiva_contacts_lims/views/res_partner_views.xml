<odoo>
  <data>
    <record model="ir.ui.view" id="view_partner_form_inherit_healthiva">
      <field name="name">res.partner.form.inherit.healthiva</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="base.view_partner_form"/>
      <field name="arch" type="xml">
        <xpath expr="//div[@class='alert alert-warning oe_edit_only']" position="before"> 
          <header>
            <button name="action_send_emr" string="Send EMR" class="oe_highlight" type="object" groups="healthiva_contacts_lims.group_contact_admin"/>
          </header>
        </xpath>
        <xpath expr="//sheet/div[@class='oe_title']/field[@name='company_type']" position="replace">
          <field name="company_type" widget="radio" class="oe_edit_only" invisible="1" options="{'horizontal': true}"/>
        </xpath>
        <xpath expr="//sheet/div[@name='button_box']" position="replace">
          <div class="oe_button_box" name="button_box">
            <button name="action_view_provider"
                type="object"
                class="oe_stat_button"
                icon="fa-globe icon"
                attrs="{'invisible': [('provider_count', '=', 0)]}">
                <field name="provider_count" widget="statinfo" string="PV1"/>
            </button>
            <button name="action_view_insurance"
                type="object"
                class="oe_stat_button"
                icon="fa-globe icon"
                attrs="{'invisible': [('insurance_count', '=', 0)]}">
                <field name="insurance_count" widget="statinfo" string="IN1"/>
            </button>
            <button name="action_view_common_order"
                type="object"
                class="oe_stat_button"
                icon="fa-globe icon"
                attrs="{'invisible': [('common_order_count', '=', 0)]}">
                <field name="common_order_count" widget="statinfo" string="ORC"/>
            </button>
            <button name="action_view_observation"
                type="object"
                class="oe_stat_button"
                icon="fa-globe icon"
                attrs="{'invisible': [('observation_count', '=', 0)]}">
                <field name="observation_count" widget="statinfo" string="OBR"/>
            </button>
            <button name="action_view_result"
                type="object"
                class="oe_stat_button"
                icon="fa-globe icon"
                attrs="{'invisible': [('result_count', '=', 0)]}">
                <field name="result_count" widget="statinfo" string="OBX"/>
            </button>
          </div>
        </xpath>
        <!-- <xpath expr="//sheet/div/h1/field[@name='name']" position="after">
        </xpath> -->
        <xpath expr="//sheet/group/group/field[@name='vat']" position="after">
          <field name="first_name"/>
          <field name="middle_name"/>
          <field name="last_name"/>
          <field name="external_pid"/>
          <field name="assigned_pid"/>
          <field name="alternate_pid"/>
          <field name="birth_date"/>
          <field name="age_years"/>
          <field name="age_months"/>
          <field name="age_days"/>
          <field name="gender"/>
          <field name="alias"/>
          <field name="race"/>
          <field name="country_code"/>
          <field name="tele_use_code"/>
          <field name="tele_equip_type"/>
          <field name="tele_address"/>
          <field name="work_phone"/>
          <field name="language"/>
          <field name="marital_status"/>
        </xpath>
        <xpath expr="//sheet/group/group/field[@name='category_id']" position="after">
          <field name="religion"/>
          <field name="is_fasting"/>
          <field name="ethnic_group"/>
          <field name="driver_license"/>
          <field name="ssn"/>
          <field name="account_number"/>
          <field name="check_digit"/>
          <field name="check_digit_scheme"/>
          <field name="bill_type"/>
          <field name="abn_flag"/>
          <field name="specimen_status"/>
          <field name="mother_maiden"/>
          <field name="mother_identifier"/>
          <field name="patient_address1"/>
          <field name="patient_address2"/>
          <field name="patient_address_city"/>
          <field name="patient_address_state"/>
          <field name="patient_address_zip"/>
        </xpath>
        <xpath expr="//sheet/notebook/page[1]" position="before">
          <page name='msh_page' string="MSH">
            <group>
                <group>
                    <field name="field_delimiter"/>
                    <field name="sending_application"/>
                    <field name="sending_facility"/>
                    <field name="receiving_application"/>
                    <field name="receiving_facility"/>                 
                    <field name="receive_date"/>                 
                </group>
                <group>
                    <field name="component_delimiter"/>
                    <field name="security"/>
                    <field name="message_type"/>
                    <field name="message_controlid"/>
                    <field name="processingid"/>
                    <field name="hl7_version"/>        
                </group>
            </group>
          </page>
        </xpath>
      </field>
    </record>

    <record model="ir.ui.view" id="view_res_partner_filter_inherit_healthiva">
