          <filter string="Patient Address Line 1" name="patient_address1"/>
          <filter string="Patient Address Line 2" name="patient_address2"/>
          <filter string="Patient Address City" name="patient_address_city"/>
          <filter string="Patient Address State" name="patient_address_state"/>
          <filter string="Patient Address zip" name="patient_address_zip"/>
        </xpath>
        <xpath expr="//search/group[@name='group_by']/filter[@name='group_country']" position="after">
          <filter string="First Name" name="group_first_name" context="{'group_by': 'first_name'}"/>
          <filter string="Middle Name" name="group_middle_name" context="{'group_by': 'middle_name'}"/>
          <filter string="Last Name" name="group_last_name" context="{'group_by': 'last_name'}"/>
          <filter string="External Patient ID" name="group_external_pid" context="{'group_by': 'external_pid'}"/>
          <filter string="Lab Assigned Patient ID" name="group_assigned_pid" context="{'group_by': 'assigned_pid'}"/>
          <filter string="Alternate Patient ID" name="group_alternate_pid" context="{'group_by': 'alternate_pid'}"/>
          <filter string="Date of Birth" name="group_birth_date" context="{'group_by': 'birth_date'}"/>
          <filter string="Age Years" name="group_age_years" context="{'group_by': 'age_years'}"/>
          <filter string="Age Months" name="group_age_months" context="{'group_by': 'age_months'}"/>
          <filter string="Age Days" name="group_age_days" context="{'group_by': 'age_days'}"/>
          <filter string="Gender" name="group_gender" context="{'group_by': 'gender'}"/>
          <filter string="Race" name="group_race" context="{'group_by': 'race'}"/>
          <filter string="Language" name="group_language" context="{'group_by': 'language'}"/>
          <filter string="Telecommunication Use Code" name="group_tele_use_code" context="{'group_by': 'tele_use_code'}"/>
          <filter string="Telecommunication Equipment Type" name="group_tele_equip_type" context="{'group_by': 'tele_equip_type'}"/>
          <filter string="Telecommunication Address" name="group_tele_address" context="{'group_by': 'tele_address'}"/>
          <filter string="Country Code" name="group_country_code" context="{'group_by':'country_code'}"/>
          <filter string="Work Phone" name="group_work_phone" context="{'group_by':'work_phone'}"/>
          <filter string="Alias" name="group_alias" context="{'group_by': 'alias'}"/>
          <filter string="Marital Status" name="group_marital_status" context="{'group_by': 'marital_status'}"/>
          <filter string="Religion" name="group_religion" context="{'group_by': 'religion'}"/>
          <filter string="Is Fasting?" name="group_is_fasting" context="{'group_by': 'is_fasting'}"/>
          <filter string="Ethinic Group" name="group_ethnic_group" context="{'group_by': 'ethnic_group'}"/>
          <filter string="Driver's License" name="group_driver_license" context="{'group_by': 'driver_license'}"/>
          <filter string="Social Security Number" name="group_ssn" context="{'group_by': 'ssn'}"/>
