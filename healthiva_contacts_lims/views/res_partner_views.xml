          <filter string="Account Number" name="account_number"/>
          <filter string="Check Digit" name="check_digit"/>
          <filter string="Check Digit Scheme" name="check_digit_scheme"/>
          <filter string="Bill Type" name="bill_type"/>
          <filter string="ABN Flag" name="abn_flag"/>
          <filter string="Status of Specimen" name="specimen_status"/>
