<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <record id="sync_document_import_contact" model="sync.document.type">
            <field name="name">Import hl7 contact</field>
            <field name="doc_code">import_contact_hl7</field>
            <field name="op_type">in-mv</field>
            <field name="active" eval="True"/>
        </record>
        <record id="sync_document_export_contact" model="sync.document.type">
            <field name="name">Export hl7 contact</field>
            <field name="doc_code">export_contact_hl7</field>
            <field name="op_type">out</field>
            <field name="active" eval="True"/>
        </record>
    </data>

    <record id="ir_cron_do_document_sync" model="ir.cron">
        <field name="name">EDI data import or export sync</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_edi_sync_action"/>
        <field name="code">model._do_doc_sync_cron()</field>
        <field name="state">code</field>
    </record>

    <record model="ir.actions.server" id="action_sale_order_edi">
        <field name="name">EDI Contact Import</field>
        <field name="model_id" ref="base_edi.model_edi_sync_action"/>
        <field name="state">code</field>
        <field name="code">
action_id = env['edi.sync.action'].search(['doc_type_id.doc_code', '=', 'import_contact_hl7'])
model._do_doc_sync_cron(sync_action_id=action_id)
        </field>
    </record>
</odoo>
